<%- include('../compartido/menu/menuAdmin')%>
<form action="/vuelo/adminAsientosVista" method="POST">
    <label for="fecha_salidaID">Fecha de salida:</label>
    <input id="fecha_salidaID" type="date" name="fecha_salida">
    <br>
    <label for="costo_boletoID">Costo del boleto:</label>
    <input id="costo_boletoID" type="number" name="costo_boleto">
